{
    "name": "mygento/module-kkm",
    "type": "magento2-module",
    "version": "2.4.1",
    "license": "OSL-3.0",
    "description": "Модуль интеграции фискальных кассовых аппаратов для Magento в соответствии с 54-ФЗ",
    "keywords": [
        "magento",
        "atol"
    ],
    "require": {
        "mygento/module-base": "~2.4.0",
        "magento/framework-message-queue": "100.4.*"
    },
    "require-dev": {
        "mygento/coding-standard": "~2.14.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Mygento\\Kkm\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": false,
            "phpro/grumphp": true
        }
    }
}
